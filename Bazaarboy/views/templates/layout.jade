doctype 5
html(lang="en")
    head
        block head
            title
                block title
                    | Bazaarboy
            script(type="text/javascript", 
                   src="{{ STATIC_URL }}js/jquery-1.10.1.min.js")
            script(type="text/javascript").
                var rootUrl = '{% url index %}';
                var csrfToken = '{{ csrf_token }}';
            script(type="text/javascript", 
                   src="{{ STATIC_URL }}js/layout.js")
    body
        block body
            div#wrapper_top
                block top
            div#wrapper_main
                block main
            div#wrapper_foot
                block foot